
void setup(){
size(500,500);

}

void draw(){

  for (int cinco = 20; cinco < 350; cinco = cinco+30) {
  
    //horizontales
    
    line(0,cinco+80,500,cinco+80);
    
    //verticales
    
  line(cinco+80,0,cinco+80,500);
  
  }
  
  for(int seis=100; seis<380; seis=seis + 30){
    for(int siete=100; siete<380;siete=siete+30){
  
    rect(siete,seis,30,30);
    }
  
  }
  
  
  
}
